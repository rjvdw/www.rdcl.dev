{
  "name": "rdcl.dev",
  "private": true,
  "type": "module",
  "workspaces": [
    "integration-tests",
    "www"
  ],
  "scripts": {
    "prepare": "husky install",
    "check:stylelint": "stylelint -f verbose 'www/src/**/*.scss'",
    "fix:stylelint": "npm run check:stylelint -- --fix",
    "check:prettier": "prettier --check .",
    "fix:prettier": "prettier --write .",
    "check:eslint": "eslint .",
    "fix:eslint": "npm run check:eslint -- --fix",
    "check": "npm run check:stylelint && npm run check:prettier && npm run check:eslint",
    "fix": "npm run fix:stylelint && npm run fix:prettier && npm run fix:eslint"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.2"
  }
}
